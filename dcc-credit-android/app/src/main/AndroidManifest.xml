                android:name="io.wexchain.android.dcc.PassportActivity"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.WebPageActivity"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.PassportCreationSucceedActivity"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.SubmitNewTokenActivity"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.AuthManageActivity"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.AboutActivity"
                android:label="关于我们"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.AffiliateActivity"
                android:theme="@style/DccLightTheme.App.Transparent"
                android:screenOrientation="portrait"/>
        <activity
                android:name="io.wexchain.android.dcc.LoanActivity"
                android:theme="@style/DccLightTheme.App.Transparent"
                android:screenOrientation="portrait"/>
    </application>

</manifest>