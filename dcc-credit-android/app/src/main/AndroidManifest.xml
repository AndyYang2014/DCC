        <activity android:name="io.wexchain.android.dcc.PassportCreationSucceedActivity"/>
        <activity android:name="io.wexchain.android.dcc.SubmitNewTokenActivity"/>
        <activity android:name="io.wexchain.android.dcc.AuthManageActivity"/>
        <activity
                android:name="io.wexchain.android.dcc.AboutActivity"
                android:label="关于我们"/>
        <activity
                android:name="io.wexchain.android.dcc.AffiliateActivity"
                android:theme="@style/DccTheme.App.Transparent">
        </activity>
        <activity android:name="io.wexchain.android.dcc.LoanActivity"
                android:theme="@style/DccTheme.App.Transparent"
