<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="io.wexchain.auth">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
            android:name="io.wexchain.android.dcc.App"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/dcc_app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="false"
            android:theme="@style/DccTheme.App"
            tools:replace="android:icon,android:label,android:theme,android:supportsRtl">
        <activity
                android:name="io.wexchain.android.dcc.LoadingActivity"
                android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="io.wexchain.android.dcc.HomeActivity"/>
        <activity
                android:name="io.wexchain.android.dcc.MyCreditActivity"
                android:label="@string/title_my_credit"/>
        <activity android:name="io.wexchain.android.dcc.SubmitPersonalInfoActivity"/>
        <activity android:name="io.wexchain.android.dcc.EditContactInfoActivity"/>
        <activity
                android:name="io.wexchain.android.dcc.SubmitIdActivity"
                android:label="@string/title_id_certification"/>
        <activity
                android:name="io.wexchain.android.dcc.SubmitBankCardActivity"
                android:label="@string/title_bank_card_certification"/>
        <activity
                android:name="io.wexchain.android.dcc.CreatePassportActivity"
                android:label="@string/create_new_digital_wallet"/>
        <activity
                android:name="io.wexchain.android.dcc.DigitalAssetsActivity"
                android:label="@string/digital_assets"/>
        <activity
                android:name="io.wexchain.android.dcc.DccExchangeActivity"
                android:label="@string/dcc"/>
        <activity
                android:name="io.wexchain.android.dcc.SubmitCommunicationLogActivity"
                android:label="@string/title_phone_carrier_certification"/>
        <activity
                android:name="io.wexchain.android.dcc.PassportImportActivity"
                android:label="@string/title_import_wallet"/>
        <activity android:name="io.wexchain.android.dcc.DigitalCurrencyActivity"/>
        <activity android:name="io.wexchain.android.dcc.DigitalTransactionDetailActivity"/>
        <activity android:name="io.wexchain.android.dcc.DigitalTransactionsActivity"/>
        <activity android:name="io.wexchain.android.dcc.CreateTransactionActivity"/>
        <activity
                android:name="io.wexchain.android.dcc.QrScannerActivity"
                android:label=""
                android:theme="@style/DccTheme.App.Transparent"/>
        <activity
                android:name="io.wexchain.android.dcc.PassportAddressActivity"
                android:label="TODO"/>
        <activity
                android:name="io.wexchain.android.dcc.IdCertificationActivity"
                android:label="@string/title_id_certification_data"/>
        <activity android:name="io.wexchain.android.dcc.BankCardCertificationActivity"
                android:label="@string/title_bank_card_certification_data"
                />
        <activity android:name="io.wexchain.android.dcc.CmLogCertificationActivity"
                android:label="@string/phone_carrier_certification_data"
                />
        <activity android:name="io.wexchain.android.dcc.PassportSettingsActivity"
                android:label="设置"
                />
        <activity android:name="io.wexchain.android.dcc.PassportRemovalActivity"
                  android:label="@string/passport_removal"/>
        <activity android:name="io.wexchain.android.dcc.EditNicknameActivity"
                android:label="修改昵称"
                />
        <activity android:name="io.wexchain.android.dcc.PassportExportActivity"
                  android:label="@string/passport_backup"/>
        <activity android:name="io.wexchain.android.dcc.ModifyPassportPasswordActivity"
                android:label="密码修改"
                />
        <activity android:name="io.wexchain.android.dcc.SearchDigitalCurrencyActivity"/>
        <activity android:name="io.wexchain.android.dcc.ChooseCutImageActivity"/>
    </application>

</manifest>